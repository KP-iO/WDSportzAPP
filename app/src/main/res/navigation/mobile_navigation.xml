<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation1"
    app:startDestination="@+id/activity_Main">

    <action
        android:id="@+id/action_blankFragment_to_home3"
        app:destination="@id/activity_Main" />
    <fragment
        android:id="@+id/activity_Main"
        android:name="com.example.wdsportz.MainFragments.Frag_Home_TempStart"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home_tempstart">
        <action
            android:id="@+id/action_HomeActivity_to_select_Teams_Frag"
            app:destination="@id/select_Teams_Frag" />
    </fragment>
    <fragment
        android:id="@+id/frag_Test_1"
        android:name="com.example.wdsportz.MainFragments.Frag_Test_1"
        android:label="fragment_frag__test_1"
        tools:layout="@layout/fragment_frag__test_1" />
    <fragment
        android:id="@+id/select_Teams_Frag"
        android:name="com.example.wdsportz.MainFragments.Frag_IniTeamSelection"
        android:label="fragment_select__teams"
        tools:layout="@layout/fragment_ini_team_selection">
        <action
            android:id="@+id/action_select_Teams_Frag_to_frag_HomePage"
            app:destination="@id/navigation2" />
    </fragment>

    <include app:graph="@navigation/navigation2" />


</navigation>