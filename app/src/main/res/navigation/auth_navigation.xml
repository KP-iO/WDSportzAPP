<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/auth_navigation"
    app:startDestination="@id/frag_login">
    <include app:graph="@navigation/main_navigation" />
    <fragment
        android:id="@+id/frag_IniTeamSelection"
        android:name="com.example.wdsportz.MainFragments.Frag_IniTeamSelection"
        android:label="fragment_ini_team_selection"
        tools:layout="@layout/fragment_ini_team_selection" >
        <action
            android:id="@+id/action_frag_IniTeamSelection_to_mobile_navigation1"
            app:destination="@+id/mobile_navigation1"
            app:enterAnim="@anim/fade_in" />
    </fragment>
    <fragment
        android:id="@+id/frag_login"
        android:name="com.example.wdsportz.MainFragments.frag_login"
        android:label="fragment_loginpage"
        tools:layout="@layout/fragment_loginpage" >
        <action
            android:id="@+id/action_frag_login_to_mobile_navigation1"
            app:destination="@id/mobile_navigation1" />
    </fragment><action android:id="@+id/action_global_frag_IniTeamSelection" app:destination="@id/frag_IniTeamSelection"/>
    <fragment
        android:id="@+id/frag_Register"
        android:name="com.example.wdsportz.MainFragments.frag_Register"
        android:label="fragment_blank_fragment2"
        tools:layout="@layout/registration" >
        <action
            android:id="@+id/action_RegisterToLogin"
            app:destination="@id/frag_login" />
    </fragment>
    <action
        android:id="@+id/globalAction_Register"
        app:destination="@id/frag_Register" /><action android:id="@+id/action_global_frag_login" app:destination="@id/frag_login"/>
</navigation>
